<%- include('partials/head.ejs') %>

  
  
      <% for(var i=0; i < postsList.length; i++) { %>
        <div class="content">
       <h1><%= postsList[i].heading %></h1>
       <h2>By <%= postsList[i].author %>, Posted <%= postsList[i].createdAt %></h2>
       <h2><%= postsList[i].content %></h2>
       
       <form action="/delete" method="post">
    
        <button name="button" type="submit" value="<%= postsList[i].id %>">Delete This Post</button>

      </form>
        </div>
    
  <% } %>
   

</div>

<%- include('partials/foot.ejs') %>